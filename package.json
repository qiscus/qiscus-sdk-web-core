{
  "name": "qiscus-sdk-javascript",
  "version": "3.3.6",
  "description": "Qiscus Web SDK Core",
  "license": "MIT",
  "main": "dist/qiscus-sdk-javascript.umd.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "lib",
    "types",
    "lib-esm"
  ],
  "scripts": {
    "serve": "serve dist -l 1235",
    "clean": "rimraf dist lib lib-esm types typings",
    "build": "run-s clean build:all",
    "build:all": "run-s build:lib:cjs build:lib:esm build:bundle build:bundle:dev",
    "build:lib:cjs": "tsc -m commonjs --outDir lib",
    "build:lib:esm": "tsc -m esnext --outDir lib-esm",
    "build:bundle": "webpack --env.production --env.target=web",
    "build:bundle:dev": "webpack --env.development --env.target=web",
    "build:watch": "run-p build:watch:*",
    "build:watch:bundle": "webpack --env.development --env.target=web --watch",
    "build:watch:cjs": "tsc --build tsconfig.json --watch",
    "build:watch:esm": "tsc -m es2015 --outDir lib-esm",
    "test": "jest",
    "dev:serve": "run-p serve build:watch:bundle",
    "release:patch": "np --any-branch --yolo patch",
    "release:alpha": "np --any-branch --yolo --tag=alpha prerelease"
  },
  "prettier": {
    "semi": false,
    "useTabs": false,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 120
  },
  "dependencies": {
    "@babel/runtime": "^7.7.4",
    "@sindresorhus/is": "^2.0.0",
    "axios": "^0.27.2",
    "core-js": "^3.4.7",
    "derivable": "^2.0.0-beta.3",
    "es6-symbol": "^3.1.3",
    "fp-ts": "^2.2.0",
    "is_js": "^0.9.0",
    "lodash.debounce": "^4.0.8",
    "lodash.flatten": "^4.4.0",
    "mqtt": "^4.2.8",
    "nanoid": "^4.0.0",
    "np": "^7.6.2",
    "pietile-eventemitter": "^1.0.0",
    "regenerator-runtime": "^0.13.9",
    "tslib": "^1.10.0",
    "xstream": "^11.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@babel/register": "^7.7.4",
    "@types/is": "0.0.21",
    "@types/jest": "^25.1.2",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.flatten": "^4.4.6",
    "@types/node": "^13.7.1",
    "@types/ws": "^7.4.7",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-macros": "^2.7.1",
    "fork-ts-checker-webpack-plugin": "^4.0.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.7.1",
    "serve": "14.1.2",
    "ts-jest": "^26.4.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^9.0.0",
    "typescript": "^3.7.5",
    "vite": "^4.0.1",
    "vitest": "^0.25.7",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
