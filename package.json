{
  "name": "qiscus-sdk-javascript",
  "version": "3.3.6",
  "description": "Qiscus Web SDK Core",
  "license": "MIT",
  "main": "dist/qiscus-sdk-javascript.umd.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "lib",
    "types",
    "lib-esm"
  ],
  "scripts": {
    "serve": "serve dist -l 1235",
    "clean": "rimraf dist lib lib-esm types typings",
    "build": "run-s clean build:all",
    "build:all": "run-s build:lib:cjs build:lib:esm build:bundle build:bundle:dev",
    "build:lib:cjs": "tsc -m commonjs --outDir lib",
    "build:lib:esm": "tsc -m esnext --outDir lib-esm",
    "build:bundle": "webpack --env.production --env.target=web",
    "build:bundle:dev": "webpack --env.development --env.target=web",
    "build:watch": "run-p build:watch:*",
    "build:watch:bundle": "webpack --env.development --env.target=web --watch",
    "build:watch:cjs": "tsc --build tsconfig.json --watch",
    "build:watch:esm": "tsc -m es2015 --outDir lib-esm",
    "test": "jest",
    "dev:serve": "run-p serve build:watch:bundle",
    "release:patch": "np --any-branch --yolo patch",
    "release:alpha": "np --any-branch --yolo --tag=alpha prerelease"
  },
  "prettier": {
    "semi": false,
    "useTabs": false,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 120
  },
  "dependencies": {
    "axios": "^0.27.2",
    "is_js": "^0.9.0",
    "lodash.debounce": "^4.0.8",
    "lodash.flatten": "^4.4.0",
    "mqtt": "^4.2.8",
    "nanoid": "^4.0.0",
    "pietile-eventemitter": "^1.0.0",
    "xstream": "^11.11.0"
  },
  "devDependencies": {
    "@types/is": "0.0.21",
    "np": "^7.6.2",
    "@types/jest": "^25.1.2",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.flatten": "^4.4.6",
    "@types/node": "^18.11.14",
    "@types/ws": "^7.4.7",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.7.1",
    "serve": "14.1.2",
    "typescript": "^3.7.5",
    "vite": "^4.0.1",
    "vitest": "^0.25.7"
  }
}
