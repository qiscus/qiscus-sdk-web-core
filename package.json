{
  "name": "qiscus-sdk-javascript",
  "version": "3.0.0-aplha.2",
  "description": "Qiscus Web SDK Core",
  "license": "MIT",
  "main": "lib/index.js",
  "typings": "typings/index.d.ts",
  "module": "lib-esm/index.js",
  "browser": "dist/qiscus-sdk-javascript.umd.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "serve": "serve dist -l 1234",
    "clean": "rimraf dist lib lib-esm",
    "build": "npm-run-all clean build:lib:cjs build:lib:esm build:bundle",
    "build:lib:cjs": "tsc --build tsconfig.json",
    "build:lib:esm": "tsc -m es2015 --outDir lib-esm",
    "build:bundle": "webpack --env.production --env.target=web"
  },
  "prettier": {
    "semi": false,
    "useTabs": false,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.4",
    "@sindresorhus/is": "^2.0.0",
    "axios": "^0.19.2",
    "core-js": "^3.4.7",
    "cuid": "^2.1.6",
    "derivable": "^2.0.0-beta.3",
    "es6-symbol": "^3.1.3",
    "fp-ts": "^2.2.0",
    "is_js": "^0.9.0",
    "lodash.debounce": "^4.0.8",
    "lodash.flatten": "^4.4.0",
    "mqtt": "^3.0.0",
    "pietile-eventemitter": "^1.0.0",
    "tslib": "^1.10.0",
    "xstream": "^11.11.0"
  },
  "devDependencies": {
    "@types/is": "0.0.21",
    "@types/jest": "^25.1.2",
